<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name></display-name>
  <servlet>
    <servlet-name>DoRegisterServlet</servlet-name>
    <servlet-class>servlet.DoRegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>doLoginServlet</servlet-name>
    <servlet-class>servlet.DoLoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>getUserListServlet</servlet-name>
    <servlet-class>servlet.GetUserListServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>getCities</servlet-name>
    <servlet-class>servlet.GetCities</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>DoRegisterServlet</servlet-name>
    <url-pattern>/servlet/doRegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>doLoginServlet</servlet-name>
    <url-pattern>/servlet/doLiginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getUserListServlet</servlet-name>
    <url-pattern>/servlet/getUserListServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getCities</servlet-name>
    <url-pattern>/servlet/getCities</url-pattern>
  </servlet-mapping>
</web-app>